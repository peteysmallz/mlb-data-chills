.circleGraphs {
  border-bottom: 1px solid #cbccce;
  border-top: 1px solid #cbccce;
  overflow: hidden;
  padding: 5px 0;
  width: 100%;
}

.circleGraph .abbrev {
  float: left;
  margin-right: 5px;
  font-size: 10px
  color: #222222
}

.circleGraph span {
  display: block;
}

.circleGraph.four {
  width: 64px;
}

.circleGraph .circle {
  background-color: #cbccce;
  border-radius: 10px;
  float: left;
  height: 10px;
  margin-right: 3px;
  width: 10px;
}

.circleGraph .description {
  display: none;
}

.circleGraph .active {
  background-color: #d00;
}
